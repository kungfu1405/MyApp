<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <title>@ViewData["Title"] - Backend</title>
    <link rel="shortcut icon" href="~/media/logos/favicon.ico" />

    <!--begin::Fonts-->
    <link rel="stylesheet" href="~/fonts/poppins/font-poppins.css" />
    <!--end::Fonts-->
    <!--begin::Global Theme Styles(used by all pages)-->
    <link href="~/plugins/global/plugins.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/plugins/custom/prismjs/prismjs.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/css/style.bundle.css" rel="stylesheet" type="text/css" />
    <!--end::Global Theme Styles-->
    <!--begin::Layout Themes(used by all pages)-->
    <link href="~/css/themes/layout/header/base/light.css" rel="stylesheet" type="text/css" />
    <link href="~/css/themes/layout/header/menu/light.css" rel="stylesheet" type="text/css" />
    <link href="~/css/themes/layout/brand/dark.css" rel="stylesheet" type="text/css" />
    <link href="~/css/themes/layout/aside/dark.css" rel="stylesheet" type="text/css" />
    <link href="~/lib/sweetalert2/sweetalert2.min.css" rel="stylesheet" type="text/css" />
    <!--end::Layout Themes-->

    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/site-media.css" />
    @await RenderSectionAsync("Styles", required: false)

    <!--begin::Global Config(global config for global JS scripts)-->
    <script src="~/js/KTAppSettings.js"></script>
    <!--end::Global Config-->
    <!--begin::Global Theme Bundle(used by all pages)-->
    <script src="~/plugins/global/plugins.bundle.js"></script>
    <script src="~/plugins/custom/prismjs/prismjs.bundle.js"></script>
    <script src="~/js/scripts.bundle.js"></script>
    <!--end::Global Theme Bundle-->
    @*<script src="//cdnjs.cloudflare.com/ajax/libs/numeral.js/2.0.6/numeral.min.js"></script>*@
</head>
<body id="kt_body" class="page-loading-enabled header-fixed header-mobile-fixed subheader-enabled subheader-fixed aside-enabled aside-fixed aside-minimize-hoverable page-loading">
    <div class="page-loader page-loader-logo">
        <img alt="Logo" class="max-h-75px" src="~/media/logos/logo-letter-4.png">
        <div class="spinner spinner-primary"></div>
    </div>
    <!--begin::Header Mobile-->
    <partial name="_Partials/_HeaderMobile" />
    <!--end::Header Mobile-->

    <div class="d-flex flex-column flex-root">
        <!--begin::Page-->
        <div class="d-flex flex-row flex-column-fluid page">
            <!--begin::Aside-->
            <partial name="_Partials/_Aside" />
            <!--end::Aside-->
            <!--begin::Wrapper-->
            <div class="d-flex flex-column flex-row-fluid wrapper" id="kt_wrapper">
                <!--begin::Header-->
                <partial name="_Partials/_Header" />
                <!--end::Header-->
                <!--begin::Content-->
                <div class="content d-flex flex-column flex-column-fluid" id="kt_content">
                    <!--begin::Subheader-->
                    <div class="subheader py-2 py-lg-4 subheader-solid" id="kt_subheader">
                        <div class="container-fluid d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
                            <div class="d-flex align-items-center flex-wrap mr-2">
                                <!--begin::Page Title-->
                                <h5 class="text-dark font-weight-bold mt-2 mb-2 mr-5">@ViewData["Title"]</h5>
                                <!--end::Page Title-->
                                <!--begin::Breadcrumb-->
                                <div class="subheader-separator subheader-separator-ver mt-2 mb-2 mr-4 bg-gray-200"></div>
                                <span class="text-muted font-weight-bold mr-4">#Home</span>
                                <!--end::Breadcrumb-->
                            </div>

                            <!--begin::Toolbar-->
                            <div class="d-flex align-items-center">
                                @await RenderSectionAsync("Toolbar", required: false)
                            </div>
                            <!--end::Toolbar-->
                        </div>
                    </div>
                    <!--end::Subheader-->
                    <!--begin::Entry-->
                    <div class="d-flex flex-column-fluid">
                        <div class="container-fluid">
                            @RenderBody()
                        </div>
                    </div>
                    <!--end::Entry-->
                </div>
                <!--end::Content-->
                <!--begin::Footer-->
                <partial name="_Partials/_Footer" />
                <!--end::Footer-->
            </div>
            <!--end::Wrapper-->
        </div>
        <!--end::Page-->
    </div>
    <!--end::Main-->
    <!-- begin::User Panel-->
    <partial name="_Partials/_UserPanel" />
    <!-- end::User Panel-->
    <!--begin::Quick Panel-->
    <partial name="_Partials/_QuickPanel" />
    <!--end::Quick Panel-->
    <!--begin::Chat Panel-->
    <partial name="_Partials/_ChatPanel" />
    <!--end::Chat Panel-->
    <!--begin::Scrolltop-->
    <div id="kt_scrolltop" class="scrolltop">
        <span class="svg-icon">
            <!--begin::Svg Icon | path:assets/media/svg/icons/Navigation/Up-2.svg-->
            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
                <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                    <polygon points="0 0 24 0 24 24 0 24" />
                    <rect fill="#000000" opacity="0.3" x="11" y="10" width="2" height="10" rx="1" />
                    <path d="M6.70710678,12.7071068 C6.31658249,13.0976311 5.68341751,13.0976311 5.29289322,12.7071068 C4.90236893,12.3165825 4.90236893,11.6834175 5.29289322,11.2928932 L11.2928932,5.29289322 C11.6714722,4.91431428 12.2810586,4.90106866 12.6757246,5.26284586 L18.6757246,10.7628459 C19.0828436,11.1360383 19.1103465,11.7686056 18.7371541,12.1757246 C18.3639617,12.5828436 17.7313944,12.6103465 17.3242754,12.2371541 L12.0300757,7.38413782 L6.70710678,12.7071068 Z" fill="#000000" fill-rule="nonzero" />
                </g>
            </svg>
            <!--end::Svg Icon-->
        </span>
    </div>
    <!--end::Scrolltop-->
    <!--begin::Sticky Toolbar-->
    <partial name="_Partials/_StickyToolbar" />
    <!--end::Sticky Toolbar-->
    <partial name="_Partials/_QuickView" />
    <script src="~/lib/sweetalert2/sweetalert2.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>
